<h2>leave your contacts, pls!</h2>

<%= form_for :contact, url: '/contacts' do |f| %> <!--contacts_path  вместо /articles-->
  <p>
    <%= f.label :email %><br/> 
     <%= f.text_field :email %>   
  </p>
  <p>
    <%= f.label :message %><br/>
     <%= f.text_area :message %>   
  </p>
  <p>
    <%= f.submit 'Send message' %>
  </p>

  <% end %>